public abstract class Gorev {

    private static String gorevtext;
    private static String tarih;
    private static String saat;
    private static String oncelik;


    public abstract  boolean isDecimal(String input);




    public static String getGorevtext() {
        return gorevtext;
    }

    public void setGorevtext(String gorevtext) {
        this.gorevtext = gorevtext;
    }

    public String getTarih() {
        return tarih;
    }

    public void setTarih(String tarih) {
        this.tarih = tarih;
    }

    public String getSaat() {
        return saat;
    }

    public void setSaat(String saat) {
        this.saat = saat;
    }

    public static String getOncelik() {
        return oncelik;
    }

    public void setOncelik(String oncelik) {
        this.oncelik = oncelik;
    }

}

